{
  "expo": {
    "name": "Plan WZIM",
    "description": "Ta aplikacja pokaże Ci twój plan zajęć.",
    "slug": "SilverTimetable",
    "privacy": "public",
    "sdkVersion": "26.0.0",
    "version": "1.1.0",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "primaryColor": "#3f51b5",
    "icon": "./assets/icon.png",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#334393"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.silver.silvertimetable",
      "supportsTablet": false,
      "isTabletOnly": false
    },
    "android": {
      "package": "com.silvernet.silvertimetable",
      "permissions": [
        "C2D_MESSAGE",
        "RECEIVE",
        "ACCESS_NETWORK_STATE",
        "INTERNET",
        "SYSTEM_ALERT_WINDOW",
        "WAKE_LOCK"
      ],
      "versionCode": 101000
    }
  }
}